{
  "name": "desktop-app",
  "version": "0.1.0",
  "private": true,
  "main": "public/electron.js",
  "author": "Marie Espinosa",
  "description": "Admin app for Nos producteurs locaux",
  "proxy": "http://localhost:3000",
  "homepage": "./",
  "dependencies": {
    "@devexpress/dx-react-chart": "^4.0.3",
    "@devexpress/dx-react-chart-material-ui": "^4.0.3",
    "@devexpress/dx-react-core": "^4.0.3",
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@mui/material": "^5.11.14",
    "@reduxjs/toolkit": "^1.9.3",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "bootstrap": "^5.2.3",
    "cypress": "^12.12.0",
    "electron-is-dev": "^2.0.0",
    "electron-packager": "^17.1.1",
    "electron-squirrel-startup": "^1.0.0",
    "react": "^18.2.0",
    "react-bootstrap": "^2.7.2",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.9.0",
    "react-scripts": "5.0.1",
    "recharts": "^2.5.0",
    "redux-persist": "^6.0.0",
    "web-vitals": "^2.1.4"
  },
  "build": {
    "appId": "com.electron.app",
    "productName": "My Electron App",
    "directories": {
      "output": "dist"
    },
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "extraResources": [
      {
        "from": "public/manifest.json",
        "to": "manifest.json"
      }
    ]
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dev": "concurrently \"cross-env BROWSER=none npm start\" \"wait-on http://localhost:3000 && electron .\"",
    "electron": "electron .",
    "electron:package:mac": "npm build && electron-builder -m -c.extraMetadata.main=build/electron.js",
    "electron:package:win": "npm run build && electron-builder -w -c.extraMetadata.main=build/electron.js",
    "electron:package:linux": "npm build && electron-builder -l -c.extraMetadata.main=build/electron.js",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "electron-build": "npm run build && electron-packager ."
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.1.1",
    "@electron-forge/maker-deb": "^6.1.1",
    "@electron-forge/maker-rpm": "^6.1.1",
    "@electron-forge/maker-squirrel": "^6.1.1",
    "@electron-forge/maker-zip": "^6.1.1",
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "electron": "^23.1.4",
    "wait-on": "^7.0.1"
  }
}
